{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "cSpell.ignorePaths": ["pnpm-lock.yaml"],
  "cSpell.words": [
    "astrojs",
    "bezier",
    "datetime",
    "evenodd",
    "focusable",
    "focusin",
    "focusout",
    "instanceof",
    "keyframes",
    "minmax",
    "nofollow",
    "noindex",
    "noreferrer",
    "onclick",
    "pintrest",
    "rarr",
    "rgba",
    "tabindex",
    "tailwindcss",
    "tiktok",
    "tsconfigs",
    "tseslint"
  ]
}
